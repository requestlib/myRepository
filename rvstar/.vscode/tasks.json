{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "gcc",
            "type": "shell",
            "command": "gcc",

            "args": ["-g",
                "${fileDirname}/test_1.c","${fileDirname}/test.c","-o","${workspaceFolder}/test",],
            "problemMatcher":{
                "owner": "gcc",
                "fileLocation": ["relative", "${workspaceRoot}"]
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
            
        }
    ]
}